{
  "manifest_version": 2,

  "name": "WooTalk_Images",
  "description": "想在聊天過程中及時插入圖片嗎？ 快來試試這個插件吧！",
  "version": "1.1.0",

  //"background": { "scripts": ["jquery-3.1.1.js","background.js"] },
  "permissions":
   [
      "tabs",
      "notifications",
      "http://*/*",
      "https://*/*"
   ],
  "page_action": {
        "default_icon": "icon.png",
        //"default_popup": "popup.html",
        "default_title": "Made By FongYeHong！"
  },
  "content_scripts" : [{
          "matches" : [ "https://wootalk.today/*"],
          "js" : ["jquery-3.1.1.js","content.js"]
  }]
}